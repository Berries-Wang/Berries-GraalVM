  ${gateTestCompileImmediately}     ${oraclejdk8}     ${gateLinuxAMD64}   {name: "gate-compiler-test-compile-immediately-8-linux-amd64", timelimit: "45:00"}
  ${gateMathStubsListener}                                                {name: "daily-hotspot-mathstubs-listener"}

